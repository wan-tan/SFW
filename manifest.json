{
    "name": "Safe For Work",
    "version": "1.0",
    "description": "Automatically deletes selected Domains from your browser history",
    "permissions": [
        "history", "storage"
    ],
    "browser_action": {
		"default_popup": "sfw.html",
		"default_icon": "img/icon16.png"
	},
	"icons": {
	     "16": "img/icon16.png",
         "48": "img/icon48.png",
        "128": "img/icon128.png"
	},
    "manifest_version": 3
}
